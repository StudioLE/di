---
source: packages/core/src/api/episode/episode_api.rs
expression: sql
---
SELECT
  "episodes"."primary_key",
  "episodes"."title",
  "episodes"."published_at",
  "episodes"."description",
  "episodes"."source_duration",
  "episodes"."image",
  "episodes"."episode",
  "episodes"."season",
  "episodes"."kind"
FROM
  "episodes"
  INNER JOIN "podcasts" ON "episodes"."podcast_key" = "podcasts"."primary_key"
WHERE
  "episodes"."primary_key" = 1
  AND "podcasts"."slug" = 'test-0'
