---
source: packages/core/src/api/podcast/podcast_api.rs
expression: sql
---
SELECT
  "episodes"."primary_key" AS "primary_key",
  "episodes"."title" AS "title",
  "episodes"."published_at" AS "published_at",
  "episodes"."source_duration" AS "source_duration",
  "episodes"."image" AS "image",
  "episodes"."episode" AS "episode",
  "episodes"."season" AS "season",
  "episodes"."kind" AS "kind"
FROM
  "episodes"
WHERE
  EXISTS (
    SELECT
      1
    FROM
      "podcasts"
    WHERE
      "podcasts"."primary_key" = 1
      AND "episodes"."podcast_key" = "podcasts"."primary_key"
  )
ORDER BY
  "episodes"."published_at" ASC
